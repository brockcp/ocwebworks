.info_intro {
  background: white;
  margin: 0 5%;
  padding: 2%;
  border-radius: 15px;
  border: 3px solid green;
  text-align: center;
}

.info_main {
  background: white;
  margin: 0 5%;
  padding: 0 2%;
  border-radius: 15px;
  text-align: center;
  border: 3px solid green;
}

.subinfo_border {
  border-bottom: 3px double #ccc;
}

.subinfo_right {
  text-align: right;
}

.underline {
  text-decoration: underline;
}

.img_flex {
  width: 100%;
  height: auto;
}

.works_img {
  width: 90%;
  border: 4px double #9988cc;
  margin: auto auto 20px auto;
  padding: 2%;
  display: inline-block;
  img {
    width: 100%;
    height: auto;
  }
}

.works_text {
  border: 1px solid green;
}

.info_main a {
  text-decoration: none;
  color: #99ccbb;
  &:hover {
    text-decoration: underline;
  }
}

