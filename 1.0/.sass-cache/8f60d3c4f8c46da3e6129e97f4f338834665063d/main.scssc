body {
  font-size: .7em;
  line-height: 1.9em;
  font-family: Lato;
}

.header-wrap {
  background: url(../img/ocbgnew.png);
  background-position: center center;
  background-size: cover;
  height: 200px;
}

.nav-wrap {
  float: right;
  width: 23%;
  margin: 2% 3% 0 0;
  text-align: center;
}

.titlepre {
  color: #FF9900;
  font-size: 1.2em;
}

.title {
  float: left;
  margin: 3% 0 0 2%;
  font-size: 2em;
  color: white;
}

nav {
  ul {
    margin: 0;
    padding: 0;
  }
  li {
    font-size: 1.2em;
    padding: 3px;
  }
  a {
    text-align: left;
    color: red;
    padding: 3px;
    border-radius: 5px;
    text-decoration: none;
    border: 1px solid white;
    transition: background-color 1s ease;
    &:hover!, &:visited! {}
    &:hover {
      color: #88E5FF;
      background-color: white;
      text-decoration: underline;
    }
  }
}

